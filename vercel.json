{
    "version": 2,
    "builds": [{
            "src": "core/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9",
        "DJANGO_SETTINGS_MODULE": "core.settings"
    }
}